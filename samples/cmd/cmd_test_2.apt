<testDef>

  <info>Sample command test</info>
  
  <shortDescription>Command Line Test #2</shortDescription>

  <test type="cmd" match="YES">
  
    <command interpreter="/bin/ksh" wdir="/tmp">ls</command>

    <input name="argument" format="literal">-l</input>
    <input name="argument" format="literal">-t</input>

    <output name="stdout" format="regexp">.*</output>
    <output name="stderr" format="literal">123</output>  <!-- fail on this --> 
    <output name="status" format="literal">0</output>

  </test>

</testDef>

