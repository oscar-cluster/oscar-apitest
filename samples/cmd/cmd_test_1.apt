<testDef>

  <info>
  Command line tester.  This should just pull up a directory listing.
  </info>

  <shortDescription>Command Line Test #1</shortDescription>

  <test type="cmd">

    <command interpreter="/bin/ksh">ls</command>

    <input name="argument" format="literal">-l</input>
    <input name="argument" format="literal">-t</input>
    <input name="argument" format="literal">-r</input>
    <input name="argument" format="literal">/tmp</input>

    <output name="stdout" format="regexp">.*</output>
    <output name="stderr" format="literal"></output> 
    <output name="status" format="literal">0</output>
  </test>
</testDef>
<!-- EOF -->
